<resources>

    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Device model (Build.MODEL) to spoof for netflix, if not empty -->
    <java-symbol type="string" name="config_netflixSpoofModel" />

    <!-- AttestationHooks -->
    <java-symbol type="array" name="config_certifiedBuildProperties" />
    <java-symbol type="string" name="config_stockFingerprint" />

    <!-- Camera -->
    <java-symbol type="array" name="config_cameraAuxPackageAllowList" />
    <java-symbol type="array" name="config_cameraAuxPackageExcludeList" />
    <java-symbol type="array" name="config_cameraHFRPrivAppList" />

    <!-- Vendor mismatch fingerprint warning -->
    <java-symbol type="string" name="system_error_vendorprint" />

    <!-- Whether or not we should show vendor mismatch message -->
    <java-symbol type="bool" name="config_show_vendor_mismatch_message" />

    <!-- Whether device supports an alt. ambient display -->
    <java-symbol type="bool" name="config_alt_ambient_display"/>

    <!-- Light / Regular style fonts -->
    <java-symbol type="string" name="config_lightFontFamily" />
    <java-symbol type="string" name="config_regularFontFamily" />

    <!-- Whether to cleanup fingerprints upon connection to the daemon and when user switches -->
    <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />

    <!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Proximity check on screen on -->
    <java-symbol type="bool" name="config_proximityCheckOnWake" />

    <!-- Proximity check timeout -->
    <java-symbol type="integer" name="config_proximityCheckTimeout" />

    <!-- Proximity check on screen on default -->
    <java-symbol type="bool" name="config_proximityCheckOnWakeEnabledByDefault" />

    <!-- ATV remote custom keys handling -->
    <java-symbol type="array" name="keyhandler_keycodes" />
    <java-symbol type="array" name="keyhandler_packages" />

    <!-- Power menu -->
    <java-symbol type="array" name="config_restartActionsList" />

    <!-- These are system packages exempted from privacy indicator -->
    <java-symbol type="array" name="config_indicatorExemptedPackageNames" />
</resources>
